{
  "name": "reactive-web-components",
  "version": "1.0.0",
  "description": "Modularized JavaScript and CSS web components for React.js applications.",
  "main": "index.js",
  "engines": {
    "node": ">= 8.6.0"
  },
  "scripts": {
    "storybook": "NODE_PATH=src start-storybook -p 9001 -c .storybook",
    "build-storybook": "NODE_PATH=src build-storybook -c .storybook -o .storybook-build",
    "build-dev": "react-scripts build",
    "prebuild": "npm run clean",
    "build": "BABEL_ENV=production babel src/components --out-dir dist --ignore node_modules/ --copy-files",
    "clean": "node_modules/.bin/rimraf build dist",
    "copy-manifest": "node_modules/.bin/copyfiles package.json .npmrc dist/",
    "predist": "npm run build && npm run copy-manifest",
    "dist": "cd dist && npm publish",
    "precommit-validate": "hooks/lint_js || (echo 'Please fix any of the above-mentioned linting errors before finalizing this commit.' && exit 1)",
    "eject": "react-scripts eject",
    "lint": "node_modules/.bin/eslint \"src/**/*.js\"",
    "start": "react-scripts start",
    "test:watch": "nodemon test/index.js",
    "test": "tape --require=jsdom --require=jsdom-global/register --require=@babel/register src/__tape__/index.js | faucet"
  },
  "pre-commit": {
    "run": [
      "precommit-validate"
    ],
    "silent": true
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:arizonatribe/reactive-web-components.git"
  },
  "keywords": [
    "react",
    "reactive",
    "redux",
    "web components",
    "javascript",
    "styled components",
    "apollo",
    "graphql",
    "component"
  ],
  "license": "Apache-2.0",
  "authors": [
    "Attainia Developers <developers@attainia.com>",
    "David Nunez <arizonatribe@gmail.com>"
  ],
  "contributors": [
    "Ana <ana.tomboulian@attainia.com>",
    "Lawrence <lawrence.grant@attainia.com>",
    "Michael <michael.tomcal@attainia.com>",
    "Jono <jono.mercier@attainia.com>",
    "Sean <sean.mchugh@attainia.com>",
    "Tim <tim.graf@attainia.com>",
    "Usha <usha.kundapur@attainia.com>"
  ],
  "dependencies": {
    "apollo-fetch": "0.7.0",
    "articles": "0.2.2",
    "ruddy": "0.3.0",
    "attasist": "0.0.29",
    "babel-polyfill": "6.26.0",
    "color": "2.0.1",
    "exact": "0.8.0",
    "fixed-data-table-2": "0.8.1",
    "formatta": "0.0.10",
    "fuzzysearch": "1.0.3",
